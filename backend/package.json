{
  "name": "ground-up-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx tsx --watch index.ts --watch-dir .",
    "migrate": "npx sequelize-cli db:migrate --config config/config.cjs",
    "seed": "npx sequelize-cli db:seed:all --config config/config.cjs",
    "undo:migrate": "npx sequelize-cli db:migrate:undo:all --config config/config.cjs",
    "undo:seed": "npx sequelize-cli db:seed:undo:all --config config/config.cjs",
    "start:prod": "NODE_ENV=production pm2 start index.ts --interpreter ./node_modules/.bin/ts-node --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sequelize-typescript": "^2.1.6",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/node": "^24.0.13",
    "ts-node": "^10.9.2"
  }
}
